{
  "name": "admin_sys",
  "version": "2.0.0-beta.1",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "PORT=8088 cross-env API_TYPE=development LOGIN_HOST=development NODE_ENV=development man-webpack dev",
    "start:pro": "PORT=8088 cross-env API_TYPE=production LOGIN_HOST=production man-webpack dev",
    "start:test": "PORT=8088 cross-env API_TYPE=localhost NO_PROXY=true LOGIN_HOST=localhost man-webpack dev",
    "build": "cross-env API_TYPE=production LOGIN_HOST=production man-webpack build",
    "build:dev": "cross-env API_TYPE=development LOGIN_HOST=development man-webpack build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "man-webpack test",
    "test:component": "man-webpack test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "@antv/data-set": "^0.8.0",
    "@babel/polyfill": "^7.0.0-beta.36",
    "antd": "^3.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-runtime": "^6.9.2",
    "classnames": "^2.2.5",
    "dva": "^2.2.3",
    "dva-loading": "^1.0.4",
    "enquire-js": "^0.2.1",
    "js-base64": "^2.5.1",
    "js-cookie": "^2.2.0",
    "less": "2.7.2",
    "lodash": "^4.17.10",
    "lodash-decorators": "^5.0.0",
    "memoize-one": "^5.0.0",
    "moment": "^2.19.1",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "rc-drawer-menu": "^0.5.0",
    "react": "^16.2.0",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-fittext": "^1.0.0",
    "react-quill": "^1.3.3",
    "rollbar": "^2.3.4",
    "url-polyfill": "^1.0.10",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.1.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "man-webpack": "^1.1.1",
    "mockjs": "^1.0.1-beta3",
    "prettier": "1.11.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.1",
    "stylelint": "^8.4.0",
    "stylelint-config-prettier": "^3.0.4",
    "stylelint-config-standard": "^18.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
